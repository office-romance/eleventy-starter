{
  "name": "zs-eleventy-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "cm": "git-cz",
    "debug": "DEBUG=Eleventy* npm run dev",
    "build": "ELEVENTY_ENV=prod npm-run-all --sequential webpack:prod start:prod ",
    "dev": "ELEVENTY_ENV=dev npm-run-all --parallel webpack:dev start:dev",
    "start:dev": "eleventy --serve",
    "start:prod": "eleventy",
    "webpack:dev": "webpack --watch --config webpack.dev.js",
    "webpack:prod": "webpack --config webpack.prod.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "autoprefixer": "^9.8.6",
    "commitizen": "^4.1.2",
    "css-loader": "^4.2.1",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "3.2.0",
    "dotenv": "^8.2.0",
    "html-minifier": "^4.0.0",
    "husky": "^4.2.5",
    "mini-css-extract-plugin": "^0.10.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "tailwindcss": "^1.6.2",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^5.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
